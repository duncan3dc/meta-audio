{
    "name": "duncan3dc/meta-audio",
    "type": "library",
    "description": "A library to read and write metadata tags to audio files",
    "keywords": ["mpg", "id3", "id3v1", "id3v2", "apetag", "apev1", "apev2", "audio", "metadata"],
    "homepage": "https://github.com/duncan3dc/meta-audio",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "duncan3dc/bom-string": "^0.1|^0.2",
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "duncan3dc/object-intruder": "^0.3",
        "duncan3dc/php-ini": "^0.1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7.7",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "duncan3dc\\MetaAudio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "duncan3dc\\MetaAudioTests\\": "tests/"
        }
    }
}
